{
  "name": "browserify-babel-demo",
  "main": "dist/module.js",
  "scripts": {
    "init": "npm install",
    "style": "grunt",
    "tpl": "tmod ./view --output ./dist",
    "distWin": "start tmod & grunt",
    "dev": "grunt & tmod",
    "release": "fis3 release -d ../Editor-release",
    "clean": "rm -rf ../Editor-release"
  },
  "devDependencies": {
    "jit-grunt": "^0.9.1",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "babelify": "^6.1.0",
    "grunt-browserify": "^3.8.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-contrib-less": "^1.1.0",
    "tmodjs": "1.0.3",

    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "style-loader": "^0.13.0",
    "css-loader": "^0.23.0",
    "less": "^2.5.3",
    "babel-preset-es2015": "^6.9.0",
    "less-loader": "^2.2.2"
  },
  "tmodjs-config": {
    "output": "./dist",
    "charset": "utf-8",
    "syntax": "simple",
    "helpers": null,
    "escape": true,
    "compress": true,
    "type": "default",
    "runtime": "template.js",
    "combo": true,
    "minify": true,
    "cache": false
  }
}
